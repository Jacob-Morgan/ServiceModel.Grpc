{
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 101,
      "MaxConcurrentUpgradedConnections": 102
    },
    "Endpoints": {
      "default": {
        "Url": "http://*:8080",
        "Protocols": "Http2"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
